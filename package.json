{
  "name": "next-template",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "scripts": {
    "preinstall": "npx check-engines && npx only-allow npm",
    "start": "run-p next-dev open",
    "build": "run-s next-build next-start",
    "serve": "run-s export server",
    "export": "run-s next-build next-export",
    "lint": "run-s next-lint prettier",
    "open": "open http://localhost:3000",
    "next-dev": "next dev",
    "next-build": "next build",
    "next-start": "next start",
    "next-lint": "next lint --dir src --fix",
    "next-export": "next export",
    "server": "browser-sync start --config ./bs-config.js",
    "prettier": "prettier --write './src/**/*.{ts,tsx}'",
    "view:tailwind": "tailwind-config-viewer -o -p 7777"
  },
  "dependencies": {
    "@hisho/utilities": "^0.2.4",
    "lodash": "^4.17.21",
    "next": "11.0.1",
    "next-seo": "^4.26.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.4",
    "what-input": "^5.2.10"
  },
  "devDependencies": {
    "@hisho/reset.css": "^1.0.1",
    "@types/browser-sync": "^3001.0.28-alpha",
    "@types/eslint": "^7.2.14",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.3.1",
    "@types/postcss-import": "^12.0.1",
    "@types/postcss-nested": "^4.2.3",
    "@types/prettier": "^2.3.2",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "autoprefixer": "^10.2.6",
    "browser-sync": "^2.27.4",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-tailwindcss": "^1.13.4",
    "next-transpile-modules": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "tailwind-config-viewer": "^1.6.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
